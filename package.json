{
  "name": "pulsar-pathfinding",
  "version": "1.1.1",
  "description": "implementation of Greedy Best-First pathfinding",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "concurrently \"lite-server\" \"npm run build-watch\"",
    "build": "webpack --config webpack.config.js",
    "build-watch": "webpack --config webpack.config.js --watch",
    "format": "prettier --single-quote --trailing-comma es5 --write $(git ls-files \"*.ts\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaduMilici/pulsar-pathfinding.git"
  },
  "author": "RaduMilici",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RaduMilici/pulsar-pathfinding/issues"
  },
  "homepage": "https://github.com/RaduMilici/pulsar-pathfinding#readme",
  "devDependencies": {
    "typescript": "^2.9.2",
    "concurrently": "^3.5.1",
    "lite-server": "^2.3.0",
    "prettier": "^1.13.5",
    "ts-loader": "^4.4.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7"
  }
}
